<Application
    x:Class="Nukepayload2.N2Engine.Shell.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Nukepayload2.N2Engine.Shell"
    xmlns:vm="using:Nukepayload2.N2Engine.Shell.ViewModels"
    RequestedTheme="Light">

    <Application.Resources>
        <ResourceDictionary>

            <vm:WelcomeViewModel x:Key="WelcomeViewModel"/>
            <vm:AboutViewModel x:Key="AboutViewModel"/>
            <vm:DesignerViewModel x:Key="DesignerViewModel"/>
            <vm:SettingsViewModel x:Key="SettingsViewModel"/>
        </ResourceDictionary>
    </Application.Resources>

</Application>
